<Application x:Class="AIMS3.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AIMS3"
			 Startup="Application_Startup"
			 DispatcherUnhandledException="Application_DispatcherUnhandledException">
	<!--StartupUri="FrontEnd/Basic/LoginWindow.xaml">-->
	<Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Resources/Dimensions.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/Dictionary-en.xaml"/>
                <ResourceDictionary Source="Resources/Dictionaries/Dictionary-fa.xaml"/>
                <ResourceDictionary Source="Resources/Authority/Operator.xaml"/>
                <ResourceDictionary Source="Resources/Authority/Admin.xaml"/>
                <ResourceDictionary Source="Resources/Authority/Expert.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>