<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
					xmlns:system="clr-namespace:System;assembly=mscorlib">

	<!--PlantView-->
    <system:Boolean x:Key="CanMapPopUp">False</system:Boolean>
	<system:Boolean x:Key="CanMapEdit">False</system:Boolean>
	<system:Boolean x:Key="CanUploadMap">False</system:Boolean>
	<system:Boolean x:Key="CanNew">False</system:Boolean>
	<system:Boolean x:Key="CanUsers">False</system:Boolean>
    <system:Boolean x:Key="CanExit">False</system:Boolean>
    <system:Boolean x:Key="CanSettings">False</system:Boolean>
	<system:Boolean x:Key="CanModules">False</system:Boolean>
	<system:Boolean x:Key="CanUtilityModule">False</system:Boolean>
	<Visibility x:Key="CanSetAll">Collapsed</Visibility>
	<Visibility x:Key="CanResetAll">Visible</Visibility>
	<Visibility x:Key="CanInitAll">Collapsed</Visibility>
	<Visibility x:Key="CanPause">Collapsed</Visibility>
</ResourceDictionary>